{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Check Environment",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(pipeline().parameters.environment,pipeline().parameters.prod)",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "Set DEV Db Namr",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "Wake_DB_Name",
								"value": {
									"value": "@pipeline().parameters.wake_dev_db_name",
									"type": "Expression"
								}
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "Set PROD DbName",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "Wake_DB_Name",
								"value": {
									"value": "@pipeline().parameters.wake_prod_db_name",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"environment": {
				"type": "string",
				"defaultValue": "dev"
			},
			"dev": {
				"type": "string",
				"defaultValue": "dev"
			},
			"prod": {
				"type": "string",
				"defaultValue": "prod"
			},
			"wake_prod_db_name": {
				"type": "string",
				"defaultValue": "Wakeschool_PROD"
			},
			"wake_dev_db_name": {
				"type": "string",
				"defaultValue": "Wakeschool_DEV"
			}
		},
		"variables": {
			"Wake_DB_Name": {
				"type": "String"
			}
		},
		"annotations": []
	}
}