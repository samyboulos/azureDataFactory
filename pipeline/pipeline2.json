{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Check Environment",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(pipeline().parameters.environment,pipeline().parameters.production)",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "Set DEV Db Namr",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "WakeSchoolDBName",
								"value": {
									"value": "@pipeline().parameters.Wakeschool_DEV",
									"type": "Expression"
								}
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "Set PROD DbName",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "WakeSchoolDBName",
								"value": {
									"value": "@pipeline().parameters.Wakeschool_PROD",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"environment": {
				"type": "string",
				"defaultValue": "dev"
			}
		},
		"annotations": []
	}
}