{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Samy-AZ-DF"
		},
		"azdf_db_server_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azdf_db_server'"
		},
		"samystoragegen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'samystoragegen2'"
		},
		"samystoragegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samystoragegen2.dfs.core.windows.net/"
		},
		"DEV_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e0c1516c-1bcf-4ea7-83df-34fa972f77e4/resourceGroups/AZ_DF/providers/Microsoft.Storage/storageAccounts/wakeschoolstorage1"
		},
		"PROD_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e0c1516c-1bcf-4ea7-83df-34fa972f77e4/resourceGroups/AZ_DF/providers/Microsoft.Storage/storageAccounts/wakeschoolstorage1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Samy-AZ-DF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azdf_db_server_connectionString": {
						"value": "[parameters('azdf_db_server_connectionString')]"
					},
					"samystoragegen2_accountKey": {
						"value": "[parameters('samystoragegen2_accountKey')]"
					},
					"samystoragegen2_properties_typeProperties_url": {
						"value": "[parameters('samystoragegen2_properties_typeProperties_url')]"
					},
					"DEV_properties_typeProperties_scope": {
						"value": "[parameters('DEV_properties_typeProperties_scope')]"
					},
					"PROD_properties_typeProperties_scope": {
						"value": "[parameters('PROD_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}